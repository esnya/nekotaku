{
  "name": "nekotaku",
  "version": "0.5.1",
  "description": "ねこ卓：モバイルフレンドリーなオンラインセッション支援ツール",
  "private": true,
  "main": "index.js",
  "repository": "https://github.com/ukatama/nekotaku.git",
  "author": "ukatama <dev.ukatama@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:webpack": "webpack",
    "build:babel": "babel -d lib --ignore src/browser -s true src",
    "clean": "rimraf lib public/assets",
    "firebase:deploy": "firebase deploy",
    "flow": "flow check",
    "lint": "eslint src",
    "start": "webpack-dev-server --progress",
    "test": "jest --coverage",
    "version": "update-changelog && git add CHANGELOG.md",
    "watch": "run-p -l watch:*",
    "watch:babel": "npm run -s build:babel -- -w",
    "watch:eslint": "esw -w src",
    "watch:jest": "jest --watch",
    "watch:server": "nodemon -d 3 -w config -w lib index.js",
    "watch:webpack": "npm run -s build:webpack -- -w"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(md)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/node_modules/babel-polyfill"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  },
  "devDependencies": {
    "autoprefixer": "^8.6.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-vue-libs": "^3.0.0",
    "eslint-watch": "^3.1.2",
    "firebase-tools": "^3.10.1",
    "flow-bin": "^0.73.0",
    "jest": "^23.1.0",
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1",
    "postcss": "^6.0.11",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "update-changelog": "^1.0.2",
    "vue-jest": "^1.3.0",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.10",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcdice-js": "^0.8.0",
    "bluebird": "^3.5.0",
    "ease-component": "^1.0.0",
    "eventemitter3": "^3.0.0",
    "express": "^4.15.4",
    "file-loader": "^1.1.5",
    "firebase": "^5.0.4",
    "google-material-color": "^1.3.1",
    "lodash": "^4.17.10",
    "log4js": "^2.3.3",
    "mdi": "^2.0.46",
    "mem": "^3.0.0",
    "moment": "^2.19.4",
    "mongodb": "^3.0.10",
    "mz": "^2.7.0",
    "scroll": "^2.0.0",
    "shortid": "^2.2.8",
    "si-tools": "^1.3.2",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "source-map-support": "^0.5.0",
    "uuid": "^3.1.0",
    "vue": "^2.5.10",
    "vue-analytics": "^5.1.1",
    "vue-dice-component": "^0.1.2",
    "vue-router": "^3.0.1",
    "vue-simple-markdown": "^1.0.6",
    "vuetify": "^0.17.4",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
